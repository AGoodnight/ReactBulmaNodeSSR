  "license": "MIT",
  "engines": {
    "node": "^8.12.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "start": "concurrently --kill-others \"nodemon --exec babel-node server.js\" \"onchange -v \"src/**/**/*.js\" \"src/**/**/*.scss\" -- npm run build\"",
    "watch": "onchange -v \"src/**/**/*.js\" \"src/**/**/*.scss\" -- npm run build",
    "webpack": "webpack -p",
    "babel_shared": "babel src/shared -d shared",
    "babel_server": "babel src/server -d server",
    "build": "npm run webpack && npm run babel_shared && npm run babel_server",
    "clean": "rm -rf assets && rm -rf shared && rm server.js",
    "reset_node_modules": "rm -rf node_modules && rm package-lock.json && yarn.lock && \"npm i\" && \"yarn install\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "node server/index.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "9.4.2",
    "babel-loader": "^8.0.4",
    "concurrently": "4.1.0",
    "css-loader": "2.0.0",
    "extract-loader": "3.1.0",
    "file-loader": "2.0.0",
    "json-loader": "^0.5.7",
    "node-sass": "4.11.0",
    "nodemon": "^1.18.4",
    "onchange": "5.2.0",
    "postcss-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.27.1",
    "webpack-cli": "^3.1.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bulma": "0.7.2",
    "hapi": "^17.6.0",
    "hapi-auth-cookie": "^9.0.0",
    "inert": "^5.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "react": "^16.5.2",
    "react-cookie": "^3.0.4",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "shortid": "^2.2.13",
    "universal-cookie": "^3.0.4"
  }
